{
  "name": "react-swipe-views",
  "version": "0.0.1",
  "description": "React Component for binded Tabs and Swipeable Views",
  "main": "./lib/SwipeViews.js",
  "scripts": {
    "build:lib": "babel ./src/SwipeViews.js -w -o ./lib/SwipeViews.js -v",
    "build:dist": "watchify -t babelify ./src/SwipeViews.js -s SwipeViews -o ./dist/SwipeViews.js -v -d"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damusnet/react-swipe-views.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "swipe",
    "views",
    "tabs"
  ],
  "author": "Damien Varron <damien@varron.fr> (https://github.com/damusnet)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damusnet/react-swipe-views/issues"
  },
  "homepage": "https://github.com/damusnet/react-swipe-views",
  "peerDependencies": {
    "react": "^0.13.0"
  },
  "devDependencies": {
    "watchify": "~3.1.0",
    "browserify": "~9.0.7",
    "babelify": "~6.0.1",
    "babel": "~5.0.8"
  }
}
